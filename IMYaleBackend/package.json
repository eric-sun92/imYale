    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "pm2": "^2.10.4",
    "redis": "^4.6.5",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "xml2js": "^0.6.2",
    "yalies": "^0.1.0"
