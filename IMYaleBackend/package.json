  "devDependencies": {
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "express-http-proxy": "^1.6.3",
    "istanbul": "^0.4.5",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
