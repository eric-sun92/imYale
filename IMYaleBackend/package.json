{
  "name": "imyale_backend",
  "version": "1.0.0",
  "description": "backend for the imyale website",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "prod": "pm2-runtime app.js ecosystem.config.js --env prod",
    "prodstop": "pm2 stop ecosystem.config.js --env prod",
    "beta": "pm2 startOrReload ecosystem.config.js --env beta",
    "betastop": "pm2 stop ecosystem.config.js --env beta",
    "createAdminUser": "node db/createAdminUser.js",
    "createDefaultRoles": "node db/createDefaultRoles.js",
    "test": "mocha test",
    "test:coverage": "istanbul cover _mocha"
